{
  "name": "icicidemo",
  "version": "1.0.0",
  "description": "A generated IBM Cloud application",
  "private": true,
  "engines": {
    "node": "^8.11.1"
  },
  "config": {
    "entrypoint": "server/server.js"
  },
  "scripts": {
    "start": "node server/server.js",
    "debug": "node --inspect=0.0.0.0:9229 server/server.js",
    "debug:brk": "node --inspect-brk=0.0.0.0:9229 server/server.js",
    "debug:legacy": "node --debug=0.0.0.0:5858 server/server.js",
    "test": "nyc mocha --exit",
    "dev": "nodemon server/server.js",
    "build-client": "react-scripts build"
  },
  "nodemonConfig": {
    "env": {
      "NODE_HEAPDUMP_OPTIONS": "nosignal"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "log4js": "^4.0.2",
    "ibm-cloud-env": "^0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "jquery": "^3.3.1",
    "popper": "^1.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "chai": "^4.2.0",
    "mocha": "^6.0.0",
    "nyc": "^13.3.0"
  }
}
