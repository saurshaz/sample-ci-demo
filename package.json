{
  "name": "icicidemo",
  "version": "1.0.0",
  "description": "A generated IBM Cloud application",
  "private": true,
  "engines": {
    "node": "^8.11.1"
  },
  "config": {
    "entrypoint": "server/server.js"
  },
  "scripts": {
    "build-server": "nodemon server/server.js",
    "debug": "node --inspect=0.0.0.0:9229 server/server.js",
    "debug:brk": "node --inspect-brk=0.0.0.0:9229 server/server.js",
    "debug:legacy": "node --debug=0.0.0.0:5858 server/server.js",
    "test": "nyc mocha --exit",
    "dev": "nodemon server/server.js",
    "build-client": "rm -rf './build' && react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start"
  },
  "nodemonConfig": {
    "env": {
      "NODE_HEAPDUMP_OPTIONS": "nosignal"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "ibm-cloud-env": "^0",
    "jquery": "^3.3.1",
    "log4js": "^4.0.2",
    "popper": "^1.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-modal": "^3.8.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "ibm-mfp-web-sdk": "^8.0.2019031118",
    "mocha": "^6.0.0",
    "nodemon": "^1.12.1",
    "nyc": "^13.3.0",
    "requirejs": "^2.3.6"
  }
}
